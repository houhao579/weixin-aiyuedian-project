<!--utils/dialog/dialog.wxml-->
<!--components/Dialog/dialog.wxml-->
<view class='wx_dialog_container' hidden="{{!isShow}}" catchtouchmove="ture">
    <view class='wx-mask'></view>
    <view class='wx-dialog'>
        <!--<view class='wx-dialog-title'>{{ title }}</view>-->
        <view class='wx-dialog-content qj-dis-c-c'>悦店优选申请获取您的微信授权</view>
        <view class='wx-dialog-footer'>
            <view class='wx-dialog-btn' catchtap='_cancelEvent'>取消</view>
            <view class="wx-dialog-btn1" >
                <button class='button-hover' open-type='getUserInfo' bindgetuserinfo="_confirmEvent">确定</button>
            </view>
        </view>
    </view>
</view>

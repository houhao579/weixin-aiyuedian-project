}
.checkbox .wx-checkbox-input {
  border-radius: 2rpx; /* 圆角 */
  width: 24rpx; /* 背景的宽 */
  height: 24rpx; /* 背景的高 */
  border:1rpx solid #C5C5C5;
  background: #fff;
  margin-right: 16rpx;
  margin-bottom: 6rpx;
}
/* 选中后的 背景样式 （背景 无边框 可根据UI需求自己修改） */
.checkbox .wx-checkbox-input.wx-checkbox-input-checked {
  border:1rpx solid #C5C5C5;
  background: #fff;
}
/* 选中后的 对勾样式 （白色对勾 可根据UI需求自己修改） */
.checkbox .wx-checkbox-input.wx-checkbox-input-checked::before {
  border-radius: 50%; /* 圆角 */
  width: 100rpx; /* 选中后对勾大小，不要超过背景的尺寸 */
  height: 100rpx; /* 选中后对勾大小，不要超过背景的尺寸 */
  line-height: 100rpx;
  text-align: center;
  font-size: 20rpx; /* 对勾大小 30rpx */
  color: #C5C5C5; /* 对勾颜色 白色 */
  background: transparent;
  transform: translate(-50%, -50%) scale(1);
  -webkit-transform: translate(-50%, -50%) scale(1);
}

/* packagepower/pages/latent/latent.wxss */

/* 邀请头部 */

.h-latent-header {
  position: relative;
  display: flex;
  flex-flow: column;
  padding: 24rpx;
  height: 134rpx;
  font-family: PingFangSC-Medium;
  font-size: 24rpx;
  background-size: 100% 100%;
}

.h-latent-btn{

  display: flex;
  align-items: center;
  justify-content: center;
  position: absolute;
  top: 50%;
  right: 50rpx;
  width: 130rpx;
  height: 50rpx;
  border-radius: 25rpx;
  color: #977419;
  font-size: 24rpx;
  background-color: #fff;
  transform: translateY(-40%);
  /* box-shadow: 2px 2px 2px #808080; */
}
.h-latent-header-text1 {
  display: flex;
  align-items: center;
  color: #977419;
}

.h-latent-header-text2 {
  color: #CFB87B;
  overflow: hidden;
}

.h-latent-header-text3{
  margin-left: 20rpx;
  margin-bottom:5rpx;
  font-size:34rpx;
}
/* 盒子 */
.h-latentlist {
  padding: 0 24rpx;
  background-color: #fff;
}
.h-latentbox {
  display: flex;
  align-items: center;
  background-color: #f9f9f9;
  padding: 24rpx;
  margin: 24rpx 0;
}

.h-latent-img {
  min-width: 80rpx;
  min-height: 80rpx;
  height: 80rpx;
  width: 80rpx;
  border-radius: 50%;
}

.h-latentbox-after {
  margin-left: 16rpx;
}

.h-latentbox-list {
  display: flex;
  align-items: center;
  font-family: PingFangSC-Regular;
  font-size: 24rpx;
  color: #c5c5c5;
}

.h-latentbox-name {
  max-width: 200rpx;
  color: #333;
  font-size: 28rpx;
  font-family: PingFangSC-Medium;
  margin-right: 16rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.h-latentbox-vip1 {
  width: 50rpx;
  height: 30rpx;
}
.h-latentbox-vip2 {
  width: 70rpx;
  height: 30rpx;
}
.h-latentbox-phone {
  color: #808080;
  margin-left: 16rpx;
}

.h-latentbox-text1 {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  color: #808080;
}

/* 无数据样式 */

.noneimgbox {
  margin: 0 auto;
  padding-top: 100rpx;
}

.nonedata-box {
  display: flex;
  flex-flow: column;
  justify-content: center;
  align-items: center;
 
  padding-top: 40rpx;
  width: 100%;
}

.nonedata-box-img {
  margin-top: 100rpx;
  width: 222rpx;
  height: 140rpx;
  overflow: hidden;
}

.nonedata-box-text {
  padding: 50rpx;
  font-size: 24rpx;
  color: #DBDBDB;
}

.h-bottom-hint {
  padding: 40rpx 0 50rpx 0;
  color: #DBDBDB;
  line-height: 28rpx;
  font-size: 24rpx;
  text-align: center;
}

.h-bottom-hint1 {
  padding: 114rpx 0 114rpx 0;
  color: #DBDBDB;
  line-height: 24rpx;
  font-size: 20rpx;
  text-align: center;
}
/*  */
.h-sharelist {
  position: fixed;
  left: 0;
  bottom: 0;
  width: 100%;
  background-color: white;
  z-index: 902;
}

.h-sharelist-list1 {
  display: flex;
  justify-content: space-between;
  align-items: center;
  font-size: 28rpx;
  height: 88rpx;
  margin-left: 72rpx;
  background: #fff;
  color: #808080;
  border-top: 1rpx solid #f3f3f3;
}

.h-sharelist-list2 {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 0 100rpx;
  height: 245rpx;
  background: #fff;
  border-top: 1rpx solid #f3f3f3;
}

.h-sharelist-box {
  margin-left: 46rpx;
  width: 200rpx;
  height: 200rpx;
  overflow: hidden;
  /* display: flex; *//* justify-content: center;
    align-items: center; */
}

.img100 {
  margin-top: 45rpx;
  margin-left: 26rpx;
  width: 86rpx;
  height: 86rpx;
}

.share-text1 {
  height: 34rpx;
  font-size: 24rpx;
  color: #404040;
  margin-left: 12rpx;
}

.share-text {
  height: 34rpx;
  font-size: 24rpx;
  color: #404040;
  margin-left: 20rpx;
}

.canvas-wh {
  width: 675rpx;
  left: 38rpx;
  top: 18rpx;
  position: fixed;
  z-index: 12;
  /* background: #fff; */
  background-size: 100% 100%;
  border-radius: 20rpx;
}

.share-head {
  width: 675rpx;
  height: 98rpx;
  margin: 0rpx !important;
  padding: 0rpx;
}

.share-goods {
  width: 675rpx;
  height: 540rpx;
  margin-top: -15rpx;
}
.share-shadow {
  z-index: 901;
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 1);
  opacity: 0.35;
}
.share-close {
  padding: 20rpx;
  height: 30rpx;
  width: 30rpx;
  margin-right: 30rpx;
}
.h-latentbox-item{
  min-width: 120rpx;
}

/* packagebuy/pages/search/search.wxss */

.h-search {
    padding: 24rpx;
    background-color: #fff;
}

.h-search-pbox1 {
    display: flex;
    justify-content: space-between;
    align-items: center;
}

.h-search-box {
    display: flex;
    align-items: center;
    padding: 0 20rpx;
    width: 596rpx;
    height: 60rpx;
    background: rgba(249, 249, 249, 1);
    border-radius: 30rpx;
}

.h-search-img {
    width: 28rpx;
    height: 28rpx;
}

.h-search-input {
    display: inline-block;
    margin-left: 20rpx;
    width: 500rpx;
    font-size: 26rpx;
    background-color: transparent;
    color: #333;
}

.h-search-inputp {
    font-size: 26rpx;
    color: #cbcbcc;
}

.h-search-text {
    display: inline-block;
    margin: 0;
    padding: 0;
    width: 98rpx;
    height: 60rpx;
    line-height: 60rpx;
    font-size: 28rpx;
    color: #808080;
    outline: none !important;
    border: none !important;
    background-color: #fff;
}

.h-search-hisbar {
    display: flex;
    justify-content: space-between;
    align-items: center;
    margin-top: 40rpx;
}

.h-search-hisbar-text {
    font-size: 28rpx;
    font-weight: bold;
    font-family: PingFangSC-Medium;
    color: #303030;
}

.h-search-hisbar-img {
    margin-right: 28rpx;
    width: 28rpx;
    height: 34rpx;
}

.h-search-history {
    margin-left: -24rpx;
}

.h-search-history-one {
    float: left;
    margin: 24rpx 0 0 24rpx;
    padding: 0 19rpx;
    width: 218rpx;
    height: 60rpx;
    line-height: 60rpx;
    text-align: center;
    font-size: 24rpx;
    background: rgba(249, 249, 249, 1);
    border-radius: 6rpx;
    color: #808080;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}
.delete1{
    width: 30rpx;
    height: 30rpx;
}
.gid-item{
    width: 100%;
    height: 80rpx;
}
.gid-text{
    font-size: 24rpx;
    margin-left: 24rpx;
}
.gid-name{
    width: 300rpx;
    height: 80rpx;
    font-size: 24rpx;
}
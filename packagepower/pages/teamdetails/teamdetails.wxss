/* packagepower/pages/teamdetails/teamdetails.wxss */



.bar1{
  position: relative;
  display: flex;
  align-items: center;
  background-color: #FFF4D8;
  height: 120rpx;
  color: #C4A452;
  font-size: 24rpx;
  font-family:  PingFangSC-Medium;
  padding: 0 24rpx;
  background-size: 100% 100%;
  font-weight: bold;
}

.h-bar-text1{

  color: #977419;
  font-size: 32rpx;
  min-width: 300rpx;
}
.h-bar-text2{

  margin: 0 10rpx;
  margin-bottom:  2rpx;
  color: #977419;
  font-size: 32rpx;
}
.h-bar-text3{
  color: #977419;
  font-size: 32rpx;
  transform: rotate(90deg)
}
.h-bar-arrow{
  margin-top: 2rpx;
  width: 24rpx;
  height: 12rpx;
}
.h-boxlist{
  background-color: #fff;
}
/* 空样式 */
.nonedata-box {
  display: flex;
  flex-flow: column;
  justify-content: center;
  align-items: center;
  padding-top: 40rpx;
  width: 100%;
}

.nonedata-box-img {
  margin-top: 100rpx;
  width: 222rpx;
  height: 140rpx;
  overflow: hidden;
}

.nonedata-box-text {
  padding: 50rpx;
  font-size: 24rpx;
  color: #DBDBDB;
}

.img100 {
  width: 100%;
  height: 100%;
}

.h-bottom-hint {
  padding: 40rpx 0 50rpx 0;
  color: #DBDBDB;
  line-height: 24rpx;
  font-size: 20rpx;
  text-align: center;
}

.vip-iocn {
  width: 50rpx;
  height: 30rpx;
}
.vip-iocn2{
  width: 64rpx;
  height: 30rpx;
}
/* 收益 */
.list{
  padding: 24rpx;
  background-color: #fff;
}
.h-minbox {
  position: relative;
  margin-top: 20rpx;
  background-color: #f9f9f9;
  font-size: 24rpx;
  color: #808080;
  border-radius: 6rpx;

}
.h-minbox1 {
  display: flex;
  align-items: flex-end;
  padding: 18rpx 32rpx;
  border-bottom: 2rpx solid #f3f3f3;
}

.h-minbox1-userimg {
  margin-right: 24rpx;
  width: 68rpx;
  height: 68rpx;
  border-radius: 50%;
}

.h-minbox1-list1 {
  margin-top: 6rpx; 
  display: flex;
  align-items: center;

}

.h-minbox1-list1-name {
  max-width: 200rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  padding-right: 10rpx;
  font-weight: bold;
}

.h-minbox1-list1-text1 {
  color: #b9b9b9;
  padding-right: 10rpx;
}

.h-minbox2-list1 {
  display: flex;
  align-items: center;
  padding-bottom: 12rpx;

}

.h-minbox2-list1-text1 {
  text-align: justify;
  min-width: 110rpx;
  color: #b9b9b9;
}

.h-minbox2-list1-text1 {
  content: '';
  display: inline-block;
}

.h-minbox2-list1-text2 {
  max-width: 460rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.h-minbox2 {
  font-weight: 500;
  padding: 24rpx 24rpx 12rpx;
}
.h-minbox1-list1-img1 {
  width: 50rpx;
  height: 30rpx;
}

.h-minbox1-list1-img2 {
  width: 64rpx;
  height: 30rpx;
}
/* 借来的样式 */
.animation-element-wrapper {
  display: flex;  
  position: fixed;
  left: 0;
  top:0;
  height: 100%;
  width: 100%;
  background-color: rgba(0, 0, 0, 0.6);
  z-index:10;
}
.animation-element {
  display: flex;
  position: fixed;
  width: 100%;
  height: 470rpx;
  bottom: 0;
  background-color: rgba(255, 255, 255, 1);
}

.animation-button {
  top:20rpx;
  width: 290rpx;
  height: 100rpx;  
  align-items:center;
}


.left-bt,.right-bt{
  color: #999999;
  display: inline-flex;  
  position: fixed;
  margin-top: 20rpx;
  height: 50rpx;
  text-align: center;
  line-height: 50rpx;
  font-size: 34rpx;
  font-family: Arial, Helvetica, sans-serif;
}

.left-bt{
  left: 30rpx;
}

.right-bt {
  right: 30rpx;
  color: red;
}

.line{
  display: block;
  position: fixed;
  height: 1rpx;
  width: 100%;
  margin-top: 89rpx; 
  background-color: #eeeeee;
}

.pickertime{
    z-index: 1000;
    position: fixed;
    left: 0;
    bottom: 0;
    height: 100vh;
    width: 100%;
    background-color: rgba(0,0,0,.3);
}
.pickertime-box{
  position: absolute;
  left: 0;
  bottom: 0;
  background-color: #fff;
  height: 300rpx;
  width: 100%;
}
.pickertime-list{
  height: 100rpx;
  text-align: center;
}
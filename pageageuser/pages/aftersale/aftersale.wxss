/* pageageuser/pages/order/order.wxss */
.t-order-list{
  width: 100%;
  min-height: 290rpx;
  background: white;
  padding: 40rpx 24rpx 0 24rpx;
}
.t-order-title{
  font-size:24rpx;
  color:#B9B9B9;
  line-height:34rpx;
}
.t-order-order{
  color: #303030;
  font-weight: bold
}
.t-order-type{
  font-size:24rpx;
  color: #FF5A5A;
  line-height:34rpx;
  float: right;
}
.t-order-list-logo{
  margin-top: 16rpx;
  width:100%;
  padding: 12rpx;
  min-height:124rpx;
  background:#F9F9F9;
}
.t-order-list-picture{
  width: 100rpx;
  height: 100rpx;
  float: left;
  margin-right: 24rpx;
}
.t-order-list-content{
  float: left;
  width: 500rpx;
  height: 100rpx;
}
.t-order-list-content-title{
  font-size:24rpx;
  color:#333;
  line-height:34rpx;
  margin-bottom: 4rpx;
  font-weight: bold
}
.t-order-list-content-sku{
  font-size:24rpx;
  color:rgba(128,128,128,1);
  line-height:34rpx;
}
.t-order-list-content-commodity{
  width: 100%;
  height: 82rpx;
  border-bottom: 1rpx solid #F9F9F9;
  padding: 24rpx 0;
  overflow: hidden
}
.t-order-list-content-price{
  font-size:24rpx;
  color:rgba(51,51,51,1);
  line-height:34rpx;
  float: right;
  margin-left: 12rpx;
  font-weight: bold
}
.t-order-list-content-price text{
  font-size:20rpx;
}
.t-order-list-content-payment{
  font-size:24rpx;
  color:rgba(197,197,197,1);
  line-height:34rpx;
  float: right;
  margin-left: 12rpx;
}
.t-order-list-content-line{
  width:2rpx;
  height:28rpx;
  float: right;
  margin-left: 12rpx;
  margin-top: 5rpx;
  background:rgba(249,249,249,1);
}
.t-order-list-content-action{
  width: 100%;
  height: 100rpx;
  overflow: hidden;
  padding: 24rpx 0;
}
.t-order-buttom-type{
  min-width:136rpx;
  height:50rpx;
  border-radius:36rpx;
  border:1rpx solid rgba(197,197,197,1);
  text-align: center;
  padding-top: 8rpx;
  font-size:24rpx;
  color:#333;
  line-height:34rpx;
  float: right;
  margin-left: 16rpx;
}
.t-order-buttom-type1{
  min-width:136rpx;
  height:50rpx;
  border-radius:36rpx;
  border:1rpx solid #FF3333;
  text-align: center;
  padding-top: 8rpx;
  font-size:24rpx;
  color:#FF3333;
  line-height:34rpx;
  float: right;
  margin-left: 16rpx;
}
.t-tianchong{
  width: 100%;
  height: 70rpx;
}
.t-order-state-content{
  position: fixed;
  left: 0;
  top: 0;
  width: 100%;
  height: 70rpx;
  padding: 18rpx 10rpx 0;
  display: flex;
  flex-wrap: nowrap;
  background: white;
}
.t-order-state-zi{
  position: relative;
  flex-grow: 1;
  font-size:28rpx;
  color:rgba(103,103,103,1);
  line-height:40rpx;
  text-align: center;
}
/*状态  */
.state{
  color: #333;
  font-weight: bold;
}
.t-state-line{
  position: absolute;
  width:48rpx;
  height:4rpx;
  background:linear-gradient(137.4deg,rgba(255,101,57,1),rgba(255,51,51,1));
  border-radius:3rpx;
  top: 62rpx;
  left: 46rpx;
  transition: all  .25s  ease-in;
}
.t-order-commodity-b{
  font-size:24rpx;
  color:#333;
  text-align: center;
  width: 100%;
  line-height:34rpx;
}
.t-order-commodity-c{
  font-size:24rpx;
  color:#FF3333;
  text-align: center;
  width: 100%;
  line-height:34rpx;
}
.t-order-noMore{
  font-size:20rpx;
  color:#808080;
  line-height:28rpx;
